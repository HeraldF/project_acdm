<%= form_for @teacher do |f| %>
	<%= f.label :full_name %> <br>
	<%= f.text_field :full_name %>
	<br>
	<%= f.label :user_name %> <br>
	<%= f.text_field :user_name %>
	<br>
	<%= f.label :email %> <br>
	<%= f.text_field :email %>
	<br>
	<%= f.label :password %> <br>
	<%= f.text_field :password %>
	<br>
	<%= f.label :password_confirmation %> <br>
	<%= f.text_field :password_confirmation %>
	<%= f.submit %>
<% end %>