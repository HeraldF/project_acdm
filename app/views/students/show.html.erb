<section id="student-show" class="container">
	<div class="page-header">
		<h1>Show <strong><%= @student.user_name %></strong> </h1>
	</div>

	<div class="form-group">
		<img src="#">
	</div>

	<div class="form-group">
		<p> <strong>Full Name:</strong><br>
			<%= @student.full_name %>
		</p>
	</div>

	<div class="form-group">
		<p> <strong>User name:</strong><br>
			<%= @student.user_name %>
		</p>
	</div>

	<div class="form-group">
		<p> <strong>E-mail:</strong><br>
			<%= @student.email %>
		</p>
	</div>

	<div class="form-group">
		<p> <strong>created at:</strong><br>
			<%= @student.created_at %>
		</p>
	</div>

	<div class="form-group">
		<p> <strong>Updated at:</strong><br>
			<%= @student.updated_at %>
		</p>
	</div>

	<%= link_to :Edit, edit_student_path, :class => "btn btn-warning btn-lg" %>
	<%= link_to :Back, students_path, :class => "btn btn-danger btn-lg" %>
</section>